import java.util.Scanner;

public class power
{
    public static void main(String[] args) {
        Scanner Sc =new Scanner(System.in);
        int a=Sc.nextInt();
        int b=Sc.nextInt();
        int result = power(a,b);
        System.out.println(result);
    }
        public static int power(int a,int b)
        {
            if(b==0)
            {
                return 1;
            }
            else
            {
                return (a*power(a,b-1));
            }
        }
        
    }
