public class SLL {

	Node head;
	class Node
	{
		int data ;
		Node next;
	}
	class list
	{
		 Node head;
		 Node tail;
		 int size;
		 
		 void addLast( int val )
		 {
			 if(size==0)
			 {
				 Node temp =new Node();
				 temp.data=val;
				 temp.next=null;
				 head=tail=temp;
				 size++;
				 
				 
			 }
		 }
	}
}
		 
		 
		
			
	
	}
}
